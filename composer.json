{
    "name": "paskuale75/yii2-summernote",
    "description": "Yii2 Summernote widget. Super simple WYSIWYG editor on Bootstrap",
    "type": "yii2-extension",
    "keywords": [
        "yii2",
        "summernote",
        "widget",
        "wysiwyg"
    ],
    "homepage": "https://github.com/paskuale75/yii2-summernote",
    "time": "2016-01-15",
    "license": "MIT",
    "authors": [
        {
            "name": "Aleksandr Zelenin",
            "email": "aleksandr@zelenin.me",
            "homepage": "http://zelenin.me",
            "role": "Developer"
        },
        {
            "name": "Joao Marques",
            "email": "joao@jjmf.com",
            "role": "Developer"
        },
        {
            "name": "simialbi",
            "email": "simi.albi@outlook.com",
            "role": "Developer"
        },
        {
            "name": "paskuale75",
            "email": "pellicanipasquale@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/paskuale75/yii2-summernote/issues",
        "source": "https://github.com/paskuale75/yii2-summernote"
    },
    "require": {
        "php": ">=7.3",
        "yiisoft/yii2": "^2.0.20",
        "bower-asset/codemirror": "@stable",
        "bower-asset/summernote": "^0.8.20"
    },
    "require-dev": {
        "aws/aws-sdk-php": "^3.148.0",
        "yiisoft/yii2-coding-standards": "~2.0"
    },
    "replace": {
        "marqu3s/yii2-summernote": "^1.0.0"
    },
    "suggest": {
        "aws/aws-sdk-php": "Needed if you like to use upload on Amazon S3"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "marqu3s\\summernote\\": "src"
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
